<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.actions.act_window" id="odoo_envioclick.package_type_window">
      <field name="name">Package Type</field>
      <field name="res_model">odoo_envioclick.package_type</field>
      <field name="view_mode">tree</field>
    </record>

    <record model="ir.ui.view" id="odoo_envioclick.package_type_list">
        <field name="name">package_type list</field>
        <field name="model">odoo_envioclick.package_type</field>
        <field name="arch" type="xml">
            <tree string="Package Type" editable="top">
                <field name="code"/>
                <field name="name"/>
                <field name="description"/>
                <field name="length"/>
                <field name="height"/>
                <field name="width"/>
                <field name="product_max"/>
                <field name="capacity"/>
                <field name="weight_box"/>
                <field name="volume"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

</odoo>