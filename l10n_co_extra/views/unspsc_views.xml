<odoo>
    <record id="product_template_UNSPSC" model="ir.ui.view">
        <field name="name">Products UNSPSC</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"></field>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="fe_dian" string="FE">
                	<group>
                		<field name="operation_type" />
                	</group>
                    <group>
                        <field name="product_UNSPSC_id" />
                        <field name="segment_name" />
                        <field name="family_name" />
                        <field name="class_name" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>